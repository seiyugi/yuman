{
  "name": "Yuman",
  "version": "2.0.4",
  "manifest_version": 2,
  "description": "Yuman collects high-resolution photos from unsplash.com and makes your new tabs more beautiful.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png"
  },
  "applications": {
    "gecko": {
      "strict_min_version": "54.0"
    }
  },
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "preferences.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
